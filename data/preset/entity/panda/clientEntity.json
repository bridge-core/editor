{
  "format_version": "1.8.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "{{PROJECT_PREFIX}}:{{IDENTIFIER}}",
      "materials": { "default": "panda" },
      "textures": {
        "default": "textures/entity/{{IDENTIFIER}}/{{IDENTIFIER}}",
        "lazy": "textures/entity/{{IDENTIFIER}}/{{IDENTIFIER}}_lazy",
        "worried": "textures/entity/{{IDENTIFIER}}/{{IDENTIFIER}}_worried",
        "playful": "textures/entity/{{IDENTIFIER}}/{{IDENTIFIER}}_playful",
        "brown": "textures/entity/{{IDENTIFIER}}/{{IDENTIFIER}}_brown",
        "weak": "textures/entity/{{IDENTIFIER}}/{{IDENTIFIER}}_sneezy",
        "aggressive": "textures/entity/{{IDENTIFIER}}/{{IDENTIFIER}}_aggressive"
      },
      "geometry": {
        "default": "geometry.{{IDENTIFIER}}"
      },
      "animations": {
        "unhappy": "animation.{{IDENTIFIER}}.unhappy",
        "sneezing": "animation.{{IDENTIFIER}}.sneezing",
        "walk": "animation.quadruped.walk",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.{{IDENTIFIER}}.baby_transform",
        "sitting": "animation.{{IDENTIFIER}}.sitting",
        "rolling": "animation.{{IDENTIFIER}}.rolling",
        "lying": "animation.{{IDENTIFIER}}.lying"
      },
      "animation_controllers": [
        { "move": "controller.animation.{{IDENTIFIER}}.move" },
        { "unhappy": "controller.animation.{{IDENTIFIER}}.unhappy" },
        { "sneezing": "controller.animation.{{IDENTIFIER}}.sneezing" },
        { "baby": "controller.animation.{{IDENTIFIER}}.baby" }
      ],
      "render_controllers": [ "controller.render.{{IDENTIFIER}}" ],
      "spawn_egg": {
        "base_color": "#000000",
          "overlay_color": "#FFFFFF"
      }
    }
  }
}