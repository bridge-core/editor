{
  "format_version": "1.8.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "{{PROJECT_PREFIX}}:{{IDENTIFIER}}",
      "materials": {
        "default": "spider",
        "invisible": "spider_invisible"
      },
      "textures": {
        "default": "textures/entity/{{IDENTIFIER}}"
      },
      "geometry": {
        "default": "geometry.{{IDENTIFIER}}"
      },
      "animations": {
        "default_leg_pose": "animation.{{IDENTIFIER}}.default_leg_pose",
        "look_at_target": "animation.{{IDENTIFIER}}.look_at_target",
        "walk": "animation.{{IDENTIFIER}}.walk"
      },
      "animation_controllers": [
        { "move": "controller.animation.{{IDENTIFIER}}.move" }
      ],
      "render_controllers": [ "controller.render.{{IDENTIFIER}}" ],
      "spawn_egg": {
        "base_color": "#000000",
				"overlay_color": "#FFFFFF"
      }
    }
  }
}