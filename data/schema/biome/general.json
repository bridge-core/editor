{
	"$schema": "http://json-schema.org/draft-07/schema",
	"definitions": {
		"noise_type": {
			"type": "string",
			"enum": [
				"beach",
				"default",
				"extreme",
				"taiga",
				"ocean",
				"mountains",
				"default_mutated",
				"deep_ocean",
				"lowlands",
				"less_extreme",
				"stone_beach",
				"swamp",
				"river",
				"mushroom",
				"highlands"
			]
		},
		"temperature": {
			"type": "string",
			"enum": ["warm", "lukewarm", "medium", "cold", "frozen"]
		},
		"vanilla_biomes": {
			"type": "string",
			"enum": [
				"bamboo_jungle",
				"bamboo_jungle_hills",
				"beach",
				"birch_forest",
				"birch_forest_hills",
				"birch_forest_hills_mutated",
				"birch_forest_mutated",
				"cold_beach",
				"cold_ocean",
				"cold_taiga",
				"cold_taiga_hills",
				"cold_taiga_mutated",
				"deep_cold_ocean",
				"deep_frozen_ocean",
				"deep_lukewarm_ocean",
				"deep_ocean",
				"deep_warm_ocean",
				"desert",
				"desert_hills",
				"desert_mutated",
				"extreme_hills",
				"extreme_hills_edge",
				"extreme_hills_mutated",
				"extreme_hills_plus_trees",
				"extreme_hills_plus_trees_mutated",
				"flower_forest",
				"forest",
				"forest_hills",
				"frozen_ocean",
				"frozen_river",
				"hell",
				"ice_mountains",
				"ice_plains",
				"ice_plains_spikes",
				"jungle",
				"jungle_edge",
				"jungle_edge_mutated",
				"jungle_hills",
				"jungle_mutated",
				"legacy_frozen_ocean",
				"lukewarm_ocean",
				"mega_taiga",
				"mega_taiga_hills",
				"mesa",
				"mesa_bryce",
				"mesa_plateau",
				"mesa_plateau_mutated",
				"mesa_plateau_stone",
				"mesa_plateau_stone_mutated",
				"mushroom_island",
				"mushroom_island_shore",
				"ocean",
				"plains",
				"redwood_taiga_hills_mutated",
				"redwood_taiga_mutated",
				"river",
				"roofed_forest",
				"roofed_forest_mutated",
				"savanna",
				"savanna_mutated",
				"savanna_plateau",
				"savanna_plateau_mutated",
				"stone_beach",
				"sunflower_plains",
				"swampland",
				"swampland_mutated",
				"taiga",
				"taiga_hills",
				"taiga_mutated",
				"the_end",
				"warm_ocean"
			]
		},
		"distributions": {
			"type": "string",
			"enum": [
				"uniform",
				"gaussian",
				"inverse_gaussian",
				"fixed_grid",
				"jittered_grid"
			]
		},
		"forced_features": {
			"properties": {
				"iterations": {
					"description": "Number of scattered positions to generate.",
					"$ref": "../molang/embeddedBeta.json"
				},
				"scatter_chance": {
					"anyOf": [
						{
							"$ref": "../feature/general.json#/definitions/chance_information"
						},
						{
							"type": "number"
						}
					]
				},
				"coordinate_eval_order": {
					"$ref": "../feature/general.json#/definitions/coordinate_eval_order"
				},
				"x": {
					"anyOf": [
						{
							"type": "string"
						},
						{
							"properties": {
								"extent": {
									"minItems": 2,
									"maxItems": 2,
									"type": "array",
									"items": {
										"anyOf": [
											{
												"type": "number"
											},
											{
												"type": "string"
											}
										]
									}
								},
								"distribution": {
									"$ref": "../feature/general.json#/definitions/distributions"
								},
								"step_size": {
									"type": "number"
								},
								"grid_offset": {
									"type": "number"
								}
							}
						}
					]
				},
				"y": {
					"$ref": "#/definitions/forced_features/properties/x"
				},
				"z": {
					"$ref": "#/definitions/forced_features/properties/x"
				},
				"places_feature": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			}
		}
	}
}
