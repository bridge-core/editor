{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "bridge.feature",
	"title": "Feature",
	"required": ["format_version"],
	"allOf": [
		{
			"properties": {
				"format_version": {
					"$ref": "../project/formatVersionEnum.json"
				}
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": [
							"1.8.0",
							"1.9.0",
							"1.10.0",
							"1.11.0",
							"1.12.0",
							"1.13.0",
							"1.14.0",
							"1.16.0",
							"1.16.100",
							"1.16.200",
							"1.16.210"
						]
					}
				}
			},
			"then": {
				"$ref": "./v1.16.0/features.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"const": "1.16.220"
					}
				}
			},
			"then": {
				"$ref": "./v1.16.220/features.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": ["1.17.0", "1.17.10", "1.17.20"]
					}
				}
			},
			"then": {
				"$ref": "./v1.17.0/features.json"
			}
		}
	]
}
