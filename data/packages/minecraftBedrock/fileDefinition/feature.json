{
	"id": "feature",
	"icon": "mdi-flower",
	"scope": "BP/features/",
	"matcher": "BP/features/*.json",
	"schema": "file:///data/packages/minecraftBedrock/schema/feature/main.json",
	"lightningCache": "feature.json",

	"definitions": {
		"blockIdentifier": [
			{
				"from": "block",
				"match": "identifier"
			},
			{
				"from": "clientBlock",
				"match": "identifier"
			}
		]
	},

	"documentation": {
		"baseUrl": "https://bedrock.dev/docs/stable/Features"
	}
}
