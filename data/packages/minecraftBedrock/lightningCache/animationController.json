[
	{ "identifier": "animation_controllers" },
	{ "state": "animation_controllers/*/states" },
	{
		"functionPath": [
			"animation_controllers/*/states/*/on_exit",
			"animation_controllers/*/states/*/on_entry"
		],
		"@map": "const func = /\\/function\\s+([aA-zZ0-9\\/]+)/g.exec(Bridge.value); if(func) return `BP/functions/${func[1]}.mcfunction`"
	}
]
